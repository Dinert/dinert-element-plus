{
    "editor.formatOnSave": false,
    "eslint.validate": [
      "javascript",
      "vue",
      "html",
      "typescript",
    ],
    "editor.detectIndentation": false,
    "files.trimTrailingWhitespace": true,
    "stylelint.validate": [
      "css",
      "scss",
      "sass",
      "vue",
      "html"
    ],
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
      "source.fixAll.stylelint": true
    },
  "vue.codeActions.enabled": false
  }